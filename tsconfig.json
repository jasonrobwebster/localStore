{
  "compilerOptions": {
    "outDir": "./dist/",
    "incremental": true,
    "tsBuildInfoFile": "node_modules/.cache/tsconfig.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
